{
    "compilerOptions": {
        "lib": [
            "ES2015"
        ],
        "target": "es2015",
        "module": "es2015",
        "strict": true,
        "typeRoots": [
            "src/types",
            "node_modules/@types",
        ],
        "outDir": "build",
        "forceConsistentCasingInFileNames": true,
        "removeComments": true,
        "jsx": "preserve",
        "noImplicitAny": false,
    }
}